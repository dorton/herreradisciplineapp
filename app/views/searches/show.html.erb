<h1>Search Results</h1>
<div class ="span6">
<table class="table table-hover table-condensed table-bordered">
  <tr>
    <th></th>
    <th>Student Name</th>
    <th>Teacher</th>
    <th>Grade</th>
    

  </tr>

<% @search.students.each do |student| %>
<tr>
    
    <td><%= link_to image_tag("#{student.stu_id}.jpg"), student %> </td>
    <td><h4><%= link_to student.full_name, student %></h4></td>
    <td><%= student.teacher %></td>
    <td><%= student.grade %></td>
    <% end %>
    
</table>
</div>
